{
  "expo": {
    "name": "Myntra Studio",
    "slug": "Myntra",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "This is project is part of the hackathon 'Myntra HackerRamp: Campus Edition 2021' organized by Myntra on Dare2compete.",
    "githubUrl": "https://github.com/HelloKunal/Myntra-Studio-Prototype/tree/main"
  }
}
